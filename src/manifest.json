{
  "bundleVersion": 1,
  "name": "Sketch Material",
  "authorEmail": "me@websiddu.com",
  "version": "3",
  "description": "Sketch material is a sketch plugin that will help you generate complex material components like tables, chips, forms etc…",
  "author": "Siddhartha Gudipati",
  "compatibleVersion": "52",
  "identifier": "com.google.material2.active",
  "commands": [{
      "script": "./handlers/actions.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "OpenDocument": "onStartup",
          "SelectionChanged.finish": "onSelectionChanged"
        }
      }
    },
    {
      "name": "Export",
      "identifier": "export",
      "script": "./commands/export.js"
    },
    {
      "name": "Toggle strip...",
      "identifier": "strip",
      "script": "./panels/strip.js"
    },
    {
      "name": "Metadata...",
      "identifier": "metadata",
      "script": "./panels/settings.js"
    },
    {
      "name": "Icons...",
      "identifier": "icons",
      "script": "./panels/styles.js",
      "shortcut": "cmd + shift + i"
    },
    {
      "name": "Color...",
      "identifier": "color",
      "script": "./panels/styles.js",
      "shortcut": "cmd + shift + c"
    },
    {
      "name": "Typography...",
      "identifier": "type",
      "script": "./panels/styles.js"
    },
    {
      "name": "Elevations...",
      "identifier": "elevation",
      "script": "./panels/styles.js"
    },
    {
      "name": "Fake data...",
      "identifier": "data",
      "script": "./panels/styles.js"
    },
    {
      "name": "Components...",
      "identifier": "components",
      "script": "./panels/styles.js"
    },
    {
      "name": "Adjust",
      "identifier": "adjust",
      "script": "./commands/adjust.js"
    }
  ],
  "menu": {
    "title": "✨ Sketch Material",
    "items": ["icons", "color", "type", "elevation", "data", "components",
      "-", "adjust", "metadata", "-", "export"
    ]
  },
  "appcast": "https://raw.githubusercontent.com/websiddu/sketch-material/master/.appcast.xml"
}
